            android:exported="false" />

        <activity
            android:name=".info.TermsAndConditionsPage"
            android:exported="false" />

        <activity
            android:name=".info.UserDataPage"
            android:exported="false" />

        <activity
            android:name=".monetize.PayPage"
            android:exported="false" />

        <activity
            android:name=".session.RegisterPage"
            android:exported="false" />

        <activity
            android:name=".session.RecoverPasswordPage"
            android:exported="false" />

        <activity
            android:name=".session.LoginPage"
            android:exported="false" />

    </application>
</manifest>
